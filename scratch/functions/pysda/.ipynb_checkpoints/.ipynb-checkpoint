    "# Make a simple request for some tabualr information\n",
    "# write the query\n",
    "# then request the results\n",
    "q = \"\"\"SELECT areasymbol, areaname, muname, musym, mukey\n",
    "from legend, mapunit\n",
    "where mapunit.lkey = legend.lkey\n",
    "AND areasymbol = 'NE109'\"\"\"\n",
    "\n",
    "tabResults = sda.tab(q=q)\n",
    "tabResults"
   ]
  },
